      <_ContentIncludedByDefault Remove="GoLondon.API.Services\obj\project.assets.json" />
      <_ContentIncludedByDefault Remove="GoLondon.API.Services\obj\project.packagespec.json" />
      <_ContentIncludedByDefault Remove="GoLondon.API.UnitTests\obj\GoLondon.API.UnitTests.csproj.nuget.dgspec.json" />
      <_ContentIncludedByDefault Remove="GoLondon.API.UnitTests\obj\project.assets.json" />
